pipeline{
    //agent{
      //  label 'dockernode'
    //}
    agent{
          
            image 'maven:alpine-test'
            label 'dockernode'
        
    }
    stages{
        stage('checkout'){
            steps{
                checkout scm
                //git credentialsId: 'pipeline', url: 'https://github.com/rameshcompany/war-1-wm-1.git'
            }
            
        }
        stage('build'){
            steps{
                sh "mvn clean install"
            }
        }
    }
}
